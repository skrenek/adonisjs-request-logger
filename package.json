{
  "name": "@skrenek/adonisjs-request-logger",
  "version": "0.1.0",
  "description": "A simple middleware module to add configurable http request logging",
  "main": "build/providers/RequestLoggerProvider.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "",
    "lint": "eslint . --ext=.ts",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "format": "prettier --write .",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc && npm run copyfiles",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "copyfiles": "copyfiles \"templates/**/*.txt\" \"instructions.md\" build"
  },
  "keywords": [
    "adonisjs",
    "request",
    "logging"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skrenek/adonisjs-request-logger.git"
  },
  "homepage": "https://github.com/skrenek/adonisjs-request-logger#adonisjs-request-logger",
  "author": "Steve Krenek (https://github.com/skrenek)",
  "bugs": {
    "url": "https://github.com/skrenek/adonisjs-request-logger/issues"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "license": "MIT",
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "16.13.1",
    "probotApps": []
  },
  "devDependencies": {
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@types/node": "^18.13.0",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "mrm": "^4.1.13",
    "np": "^7.6.3",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "files": [
    "build/adonis-typings",
    "build/src",
    "build/providers",
    "build/templates",
    "build/instructions.md",
    "build/instructions.js"
  ],
  "adonisjs": {
    "instructionsMd": "./build/instructions.md",
    "instructions": "./build/instructions.js",
    "providers": [
      "@skrenek/adonisjs-request-logger"
    ],
    "templates": {
      "basePath": "./build/templates"
    },
    "types": "@skrenek/adonisjs-request-logger"
  },
  "dependencies": {
    "@adonisjs/sink": "^5.4.2"
  }
}
